 {
  "name": "pkg-name",
  "type": "module",
  "version": "0.0.0",
  "description": "",
  "author": "winches",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "release": "npm publish"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/node": "^18.7.18",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "lint-staged": "^13.1.2",
    "simple-git-hooks": "^2.8.1",
    "unbuild": "^0.8.11",
    "typescript": "^5.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ]
  }
}
