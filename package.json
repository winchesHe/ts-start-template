{
  "name": "@winches/ts-start",
  "type": "module",
  "version": "0.2.1",
  "description": "",
  "author": "winches",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "bin": {
    "ts-start": "dist/bin.js"
  },
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "tsup --watch",
    "build": "tsup-node",
    "lint": "eslint --cache --ext ts,tsx,vue .",
    "prepublishOnly": "npm run build",
    "start": "esno src/bin.ts",
    "test": "ts-start",
    "typecheck": "tsc --noEmit",
    "changlog": "npx conventional-changelog -p angular -i CHANGELOG.md -s --commit-path .",
    "release": "bumpp --execute=\"npm run changlog\" --all"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "template",
    ".gitignore",
    ".npmrc"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "inquirer": "^9.1.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.7.18",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "lint-staged": "^13.1.2",
    "prettier": "^3.0.1",
    "rimraf": "^4.1.2",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ]
  }
}
